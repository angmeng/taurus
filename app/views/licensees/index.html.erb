<p id="notice"><%= notice %></p>
<div class="container">
<h1>Licensees</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Branch</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @licensees.each do |licensee| %>
      <tr>
        <td><%= licensee.name %></td>
        <td><%= licensee.email %></td>
        <td><%= licensee.password %></td>
        <td><%= licensee.branch.name %></td>
        <td><%= licensee.teacher.name %></td>
        <td><%= link_to 'Show', licensee %></td>
        <td><%= link_to 'Edit', edit_licensee_path(licensee) %></td>
        <td><%= link_to 'Destroy', licensee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p class="center-align"><%= link_to 'New Licensee', new_licensee_path %></p>
</div>